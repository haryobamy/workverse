{
    "compilerOptions": {
      "target": "ES6",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noFallthroughCasesInSwitch": true,
      "module": "NodeNext",
      "moduleResolution": "NodeNext",
      "isolatedModules": true,
      "noEmit": true,
    
    },
 
  }